{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/CustomButton.jsx",
    "app/(auth)/sign-in.jsx",
    "components/FormField.jsx",
    "app/index.tsx",
    "app/(auth)/_layout.jsx",
    "app/(tabs)/_layout.jsx"
  ]
}